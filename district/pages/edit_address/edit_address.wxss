/* pages/managAddress/managAddress.wxss */
@import '../../common.wxss';
page{
  background-color: #f5f5f5;
}
.main{
  width: 100%;
   background-color: #ffffff;
   margin-top: 20rpx;
}
.list{
  width: 100%;
  height: 88rpx;
  line-height: 88rpx;
  border-bottom: 1rpx solid #dddddd;
}
.list text{
  width: 150rpx;
  font-size: 28rpx;
  color: #666666;
  margin-left: 20rpx;
}
.list input,.list navigator{
  display: inline-block;
  margin-left: 40rpx;
  font-size: 32rpx;
  color: #333333;
}
.last{
  border: none;
}
.default{
  width: 100%;
  background-color: #ffffff;
  margin-top: 20rpx;
  height: 88rpx;
}
.default image{
  width: 40rpx;
  height: 40rpx;
   display: inline-block;
  margin-left: 20rpx;
}
.default-text{
  display: inline-block;
  margin-left: 20rpx;
  font-size: 32rpx;
  color: #333333;

}
.default-radio{
   display: inline-block;
  margin-left: 20rpx;
}
.save{
  position: fixed;
  bottom: 0rpx;
  width: 100%;
  height: 96rpx;
  margin: 0rpx auto;
  text-align: center;
  line-height: 96rpx;
  background-color: #fe791d;
}
.save text{
  color: #ffffff;
  font-size: 36rpx;
}
.picker{
  width: 750rpx;
}
#region{
  font-size: 32rpx;
  color: #333333;
  margin-left: 50rpx;
}