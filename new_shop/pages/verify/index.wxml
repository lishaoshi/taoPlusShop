<!--pages/verify/index.wxml-->
<view class="container">
  <view class="title">
    <text>请输入券码</text>
  </view>
  <view class="scan_box">
    <input class="input" bindblur="getCode" value="{{value}}"></input>
    <view class="right" bindtap="openScanCode">
      <image src="../../images/scan.svg"></image>
      <text>点击扫描二维码</text>
    </view>
  </view>

  <view class="btn" bindtap="queryOrder">
    <text>确认</text>
  </view>
</view>
