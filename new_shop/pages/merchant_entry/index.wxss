/* pages/merchant_entry/index.wxss */
.container > image {
  width: 100%;
  height: 300rpx;
}
.tel {
  font-size: 24rpx;
  color:rgba(187, 184, 184, 0.74);
  padding: 20rpx 30rpx;
}
.tel > text {
  color: rgb(51, 115, 235);
}
.list-box image {
  width: 56rpx;
  height: 56rpx;
  margin-right: 50rpx;
}
.list-box {
  display: flex;
  flex-direction: column;
  align-items: center;
}
.list {
  display: flex;
  align-items: center;
  padding: 40rpx 60rpx;
  border: 1px solid #000;
  margin-bottom: 20rpx;
}
.btn {
  position: absolute;
  bottom: 20rpx;
  left: 50%;
  width: 80%;
  margin-left: -40%;
  background: rgb(66, 133, 235);
  text-align: center;
  color: #fff;
  border-radius: 42rpx;
  padding: 20rpx 0;
}