.bg_title {
  background: #e52e31;
  padding: 10rpx 30rpx;
  margin-right: 20rpx;
  border-radius: 6rpx;
  color: #fff;
}

.bg_title {
  font-size: 30rpx;
}

.dis-flex {
  display: flex;
  display: -webkit-flex;
}

.flex-justify {
  justify-content: space-between;
  -webkit-justify-content: space-between;
}

page {
  background: #fff;
  overflow: hidden;
}

.center {
  text-align: center;
  width: 175rpx;
}

