.top-region {
  padding: 0 30rpx;
  padding-top: 20rpx;
  z-index: 99;
  opacity: 0.8;
  -webkit-transition: 0.4s;
  -moz-transition: 0.4s;
  transition: 0.4s;
}

.search {
  position: relative;
  border-radius: 100rpx;
  padding-right: 28rpx;
}

.search .search-input {
  width: 560rpx;
  height: 58rpx;
  line-height: 58rpx;
  border-radius: 100rpx;
  background: #f2f2f2;
  font-size: 22rpx;
  color: #ddd;
  padding-left: 56rpx;
}

.search .search-icon {
  position: absolute;
  top: 50%;
  left: 26rpx;
  margin-top: -14rpx;
  width: 28rpx;
  height: 28rpx;
  z-index: 10;
}

