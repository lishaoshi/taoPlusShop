/* pages/withdraw/withdraw.wxss */
@import '../../../common.wxss';
page {
    background: #f5f5f5;
    padding: 20rpx;
}

.withdraw {
    box-sizing: border-box;
    background: #fff;
    width: 710rpx;
}

.withdraw-head {
    
    padding: 0 25rpx;
    line-height: 88rpx;
    font-size: 32rpx;
    color: #333;
}

.withdraw-bank {
    font-size: 28rpx;
    color: #999;
}

.withdraw-content {
    padding: 40rpx 20rpx 0;
}

.withdraw-title {
    font-size: 30rpx;
    line-height: 1;
    color: #999;
}

.input-box {
    font-size: 46rpx;
    color: #333;
    padding: 84rpx 0 77rpx 0;
}

.withdraw-input {
    border: none;
    font-size: 78rpx;
    height: 78rpx;
    width: 500rpx;
    margin-left: 10rpx;

}

.withdraw-foot {
    height: 88rpx;
    line-height: 88rpx;
    padding-left: 20rpx;
    font-size: 30rpx;
    color: #999;
}

.withdraw-foot .blue {
    color: #327ff4;
}
.bankId{
  color: #000000;
}